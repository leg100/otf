{{ template "layout" . }}

{{ define "title" }}Workspaces{{ end }}

{{ define "sidebar" }}{{ template "me_sidebar" . }}{{ end }}

{{ define "content" }}
  <table class="table">
    <thead>
    <tr>
      <th scope="col">Name</th>
    </tr>
  </thead>
  <tbody>
    {{ range .Content.List.Items }}
      <tr>
          <td><a href="{{ $.Path "getWorkspace" "organization_name" .Organization.Name "workspace_name" .Name }}">{{ .Name }}</a></td>
        </tr>
      {{ end }}
    </tbody>
  </table>
{{ end }}
