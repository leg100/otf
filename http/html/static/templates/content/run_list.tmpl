{{ template "layout" . }}

{{ define "title" }}Runs{{ end }}

{{ define "content" }}
  <table class="table">
    <thead>
      <tr>
        <th scope="col">ID</th>
      </tr>
    </thead>
    <tbody>
      {{ range .Content.List.Items }}
      <tr>
        <td><a href="{{ $.Path "getRun" "organization_name" $.Content.Options.OrganizationName "workspace_name" $.Content.Options.WorkspaceName "run_id" .ID }}">{{ .ID }}</a></td>
      </tr>
      {{ end }}
    </tbody>
  </table>
{{ end }}
